        "contador = 0\n",
        "soma = 0\n",
        "pares = 0\n",
        "impares = 0\n",
        "\n",
        "maior = None\n",
        "menor = None\n",
        "\n",
        "while True:\n",
        "    numero = int(input(\"Digite um n√∫mero (0 para parar): \"))\n",
        "\n",
        "    if numero == 0:\n",
        "        break\n",
        "\n",
        "    contador += 1\n",
        "    soma += numero\n",
        "\n",
        "    if numero % 2 == 0:\n",
        "        pares += 1\n",
        "    else:\n",
        "        impares += 1\n",
        "\n",
        "    if maior is None or numero > maior:\n",
        "        maior = numero\n",
        "\n",
        "    if menor is None or numero < menor:\n",
        "        menor = numero\n",
        "\n",
        "\n",
        "if contador > 0:\n",
        "    media = soma / contador\n",
